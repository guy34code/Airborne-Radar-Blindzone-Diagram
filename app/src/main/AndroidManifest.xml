        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_radar_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_radar_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".UpdateDetails"></activity>
        <activity android:name=".DisplyIndividualRadar" />
        <activity android:name=".popup"/>
        <activity android:name=".Firstscreen" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DisplayRadar" />
        <activity android:name=".Graph" />
        <activity android:name=".DynamicVariables" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>